        <Image x:Name="LocalIdle" Height="75" Margin="786,383,0,0" Width="280" Source="/Ascendancy;component/Resources/Images/UserControlResources/Multiplayer/MultiplayerStarter/LocalIdle.png" VerticalAlignment="Top" HorizontalAlignment="Left" MouseEnter="UserControlButton_MouseEnter" MouseLeave="UserControlButton_MouseLeave" MouseLeftButtonDown="UserControlButton_MouseDown" MouseLeftButtonUp="LocalIdle_MouseLeftButtonUp"/>
        <Image x:Name="OnlineClick" Height="75" Margin="368,383,0,0" Width="280" Source="/Ascendancy;component/Resources/Images/UserControlResources/Multiplayer/MultiplayerStarter/OnlineClick.png" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Image x:Name="OnlineHover" Height="75" Margin="368,383,0,0" Width="280" Source="/Ascendancy;component/Resources/Images/UserControlResources/Multiplayer/MultiplayerStarter/OnlineHover.png" VerticalAlignment="Top" HorizontalAlignment="Left"/>

             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="1440" Height="900">
    <UserControl.Background>
        <ImageBrush ImageSource="/Ascendancy;component/Resources/Images/UserControlResources/ExitConfirmation/QuitBackdrop.png" Stretch="Uniform"/>
    </UserControl.Background>
    <Grid x:Name="MultiplayerStarterGrid" Width="1440" Height="900">
        <Image x:Name="LocalClick" Height="75" Margin="786,383,0,0" Width="280" Source="/Ascendancy;component/Resources/Images/UserControlResources/Multiplayer/MultiplayerStarter/LocalClick.png" VerticalAlignment="Top" HorizontalAlignment="Left"/>

<UserControl x:Name="GameCompleteUserControlView" x:Class="Ascendancy.User_Controls.GameCompleteUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Width="1440" Height="900">
    <UserControl.Background>
        <ImageBrush ImageSource="/Ascendancy;component/Resources/Images/UserControlResources/ExitConfirmation/QuitBackdrop.png" Stretch="Uniform"/>
    </UserControl.Background>
    <Grid x:Name="GameCompleteGrid" Width="1440" Height="900">
        <Label x:Name="CompletionLabel" Content="You won!&#xD;&#xA;You lost!&#xD;&#xA;Who knows!&#xD;&#xA;Idk bro!" Margin="503,294,522,376" Height="230" Width="415" FontSize="36" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Image x:Name="OkayClick" Height="75" Margin="572,583,0,0" Width="280" Source="/Ascendancy;component/Resources/Images/UserControlResources/Options/OkayClick.png" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Image x:Name="OkayHover" Height="75" Margin="572,583,0,0" Width="280" Source="/Ascendancy;component/Resources/Images/UserControlResources/Options/OkayHover.png" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <Image x:Name="OkayIdle" Height="75" Margin="572,583,0,0" Width="280" Source="/Ascendancy;component/Resources/Images/UserControlResources/Options/OkayIdle.png" VerticalAlignment="Top" HorizontalAlignment="Left"  MouseLeftButtonUp="OkayIdle_MouseLeftButtonUp" MouseDown="UserControlButton_MouseDown" MouseEnter="UserControlButton_MouseEnter" MouseLeave="UserControlButton_MouseLeave"/>

    </Grid>
</UserControl>
